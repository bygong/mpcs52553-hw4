<% dt = Actor.all() %>

<div class="row">
  <div class="col-sm-3">

  </div>
  <div class="col-sm-8 well">
    <div class="page-title">
      <h1>
          Actors
      </h1>

      <% dt.each do |act| %>
        <p><%= link_to "#{act.name}", "http://localhost:3000/actors/#{act.id}" %></p>
      <% end %>

    </div>
  </div>
</div>
